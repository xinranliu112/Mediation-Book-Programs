TITLE:  Chapter 13 Three Factor Latent Variable Model Synthetic Reliability Estimation;

DATA:
  FILE IS Chap13_Exp1 Raw Data.txt;

VARIABLE:
  NAMES ARE id coach1 coach2 coach3 severe1 severe2 severe3 intent1
  intent2 intent3;
  USEVARIABLES ARE coachs severes intents;

DEFINE:
  coachs = coach1 + coach2 + coach3;
  severes = severe1 + severe2 + severe3;
  intents = intent1 + intent2 + intent3;

ANALYSIS:
  TYPE IS GENERAL;
  ESTIMATOR IS ML;
  ITERATIONS = 1000;
  CONVERGENCE = 0.00005;

Model:
   severe by severes@1; severes@.2828;
   coach by coachs@1; coachs@.1784;
   intent by intents@1; intents@.5629;
   intent on severe coach;
   severe on coach;

Model indirect:
intent ind coach;

OUTPUT:  SAMPSTAT STANDARDIZED CINTERVAL TECH4;
